    "clean": "rm -rf dist"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^5.19.1",
    "@trustless/shared": "workspace:*",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
